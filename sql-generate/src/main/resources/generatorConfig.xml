<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <properties resource="generatorConfig.properties"/>

    <classPathEntry location="${jdbc_driverPath}"/>

    <context id="my" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <!-- 取消注释的生成 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="${jdbc_driverClassName}" connectionURL="${jdbc_url}" userId="${jdbc_username}"
                        password="${jdbc_password}"/>

        <javaModelGenerator targetPackage="${modelPackage}" targetProject="${targetProject}">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="${mappingPackage}" targetProject="${targetProject}">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="${mapperPackage}" targetProject="${targetProject}" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="idss_ims_admin" tableName="city" domainObjectName=""
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">

            <!--是否使用数据库表的列名作为model的字段名字-->
            <property name="useActualColumnNames" value="false"/>

            <!--生成insert后的selectKey标签-->
            <generatedKey column="id" sqlStatement="MySql" identity="true" />

            <ignoreColumn column="FRED" />
        </table>

        <table schema="idss_ims_admin" tableName="city_org_mapping" domainObjectName=""
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">

            <!--是否使用数据库表的列名作为model的字段名字-->
            <property name="useActualColumnNames" value="false"/>

            <!--生成insert后的selectKey标签-->
            <generatedKey column="id" sqlStatement="MySql" identity="true" />

            <ignoreColumn column="FRED" />
        </table>

        <table schema="idss_ims_admin" tableName="menu" domainObjectName=""
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">

            <!--是否使用数据库表的列名作为model的字段名字-->
            <property name="useActualColumnNames" value="false"/>

            <!--生成insert后的selectKey标签-->
            <generatedKey column="id" sqlStatement="MySql" identity="true" />

            <ignoreColumn column="FRED" />
        </table>


        <table schema="idss_ims_admin" tableName="menu_attr" domainObjectName=""
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">

            <!--是否使用数据库表的列名作为model的字段名字-->
            <property name="useActualColumnNames" value="false"/>

            <!--生成insert后的selectKey标签-->
            <generatedKey column="id" sqlStatement="MySql" identity="true" />

            <ignoreColumn column="FRED" />
        </table>


        <table schema="idss_ims_admin" tableName="organization" domainObjectName=""
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">

            <!--是否使用数据库表的列名作为model的字段名字-->
            <property name="useActualColumnNames" value="false"/>

            <!--生成insert后的selectKey标签-->
            <generatedKey column="id" sqlStatement="MySql" identity="true" />

            <ignoreColumn column="FRED" />
        </table>


        <table schema="idss_ims_admin" tableName="shop" domainObjectName=""
               enableCountByExample="false"
